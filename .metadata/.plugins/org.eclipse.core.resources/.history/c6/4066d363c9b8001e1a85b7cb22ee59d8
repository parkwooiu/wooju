 Scanner scan = new Scanner(System.in);
       int  a   =  scan.nextInt();
       int  b = scan.nextInt();
       int  c = scan.nextInt();
       
       if (a>b)
       if (a>c)
    	   System.out.println(a+ "는 가장 큰 정수 입니다");
