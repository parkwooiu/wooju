package ex03;

import java.util.Scanner;

import ex02.Scanner01;

public class IfElseIfExam03 {

	public static void main(String[] args) {
		
     System.out.println("당신의 나이름 입력하세요");    
     
     Scanner s = new Scanner(System.in);
     int age = s.nextInt();
     
     if (age >= 18)
    	 System.out.println("당신의 나이는 18세 이상입나다");
     System.out.println("당신은 투표할 자격이 있습니다");
     
	
	
		
	
			
	
		
		
				
		

	}

}
