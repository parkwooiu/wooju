public class IfElseIfExam03 {

	public static void main(String[] args) {
		
    int num = 120;
    
   if ( num <= 100 );
    System.out.println("100보다 작습니다");
   
    
   
   System.out.println("100보다 큽니다");
  
	
		
	
			
	
		
		
				
		

	}

}
