package ex01;

public class Switch5 {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
		
		char grade = 100;
		switch (grade / 10 ) {
		case 10 :
		System.out.println("합격");
		case 9 :
			System.out.println("아구 이뻐라");
		case 8 :
			System.out.println("잘했어 아주");
		case 7 :
			System.out.println("잘했네");
		case 6 :
			System.out.println("좀만 더 해보자");
		case 5 : 
			System.out.println("아쉽네");
			default
			System.out.println("이건 좀 아니잖아");
		
		}
		


	
	}
}
